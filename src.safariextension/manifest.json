{
  "name": "Feedly Notifier Plus",
  "short_name": "ifnplus",
  "description": "Feedly Notifier Plus is a badged powered toolbar notifier for feedly.com, a productive way to organize, read and share the content of your favorite feeds, blogs and news sites.",
  "author": "Jeremy Schomery",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "background",
    "webRequest",
    "http://feedly.com/*",
    "https://feedly.com/*",
    "http://mybrowseraddon.com/feedly.html"
  ],
  "web_accessible_resources": [],
  "browser_action": {
    "default_icon": "data/icons/disabled/24.png"
  },
  "background": {
    "page": "lib/chrome/background.html"
  },
  "content_scripts": [{
    "matches": ["http://feedly.com/*", "https://feedly.com/*"],
    "js": ["data/content_script/chrome/chrome.js", "data/content_script/inject.js"],
    "run_at": "document_start",
    "all_frames": false
  }],
  "options_page": "data/options/index.html",
  "homepage_url": "http://mybrowseraddon.com/feedly.html",
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  }
}
